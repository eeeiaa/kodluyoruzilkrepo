import java.util.Scanner;

public class project {
    public static void main(String[] args) {
        int n;
        Scanner inp = new Scanner(System.in);
        System.out.print("Bir sayi giriniz : ");
        n = inp.nextInt();

        for (int i = 1; i <= n; i *= 4) {
            System.out.println(i);
        }
    }
}
