public class project {
    public static void main(String[] args) {
        int n1 = 0, n2 = 1, n3, i, counter = 10;

        for (i = 2; i < counter; ++i) {
            n3 = n1 + n2;
            System.out.println(n1 + " + " + n2 + " = " + n3);
            n1 = n2;
            n2 = n3;
        }
    }
}
